{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "promise",
            "program": "${workspaceFolder}/es6/promise.js"
        },
    {
        "type": "node",
        "request": "launch",
        "name": "Electron Main",
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
        "program": "${workspaceRoot}/main.js",
        "protocol": "legacy"
    },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceRoot}"
        }
    ]
}